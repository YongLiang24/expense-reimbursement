<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MyServlet</servlet-name>
  	<display-name>MyServlet</display-name>
  	<description></description>
  	<servlet-class>com.yongliang.myservlet.MyServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>SecondServlet</servlet-name>
  	<display-name>SecondServlet</display-name>
  	<description></description>
  	<servlet-class>com.yongliang.myservlet.SecondServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ThirdServlet</servlet-name>
  	<display-name>ThirdServlet</display-name>
  	<description></description>
  	<servlet-class>com.yongliang.myservlet.ThirdServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmpLoginServlet</servlet-name>
  	<display-name>EmpLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.yongliang.myservlet.EmpLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeHome</servlet-name>
  	<display-name>EmployeeHome</display-name>
  	<description></description>
  	<servlet-class>com.yongliang.myservlet.EmployeeHome</servlet-class>
  </servlet>
  <servlet>
